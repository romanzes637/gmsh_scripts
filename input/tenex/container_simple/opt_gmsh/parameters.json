          "mapping": {"metadata": {"run": {"options": {"Mesh.MeshSizeMin": "{size_min}"}}}}}
      },
      {"key": "size_max", "class": "action.Feature",
        "pre_call": {"class": "action.set.Continuous", "low": 0.1, "high": 1},
        "post_call": {"class": "action.get.Json", "path": "gmsh/container.json",
          "mapping": {"metadata": {"run": {"options": {"Mesh.MeshSizeMax": "{size_max}"}}}}}
      },
      {"key": "size_factor", "class": "action.Feature",
        "pre_call": {"class": "action.set.Continuous", "low": 0.1, "high": 2.0},
        "post_call": {"class": "action.get.Json", "path": "gmsh/container.json",
          "mapping": {"metadata": {"run": {"options": {"Mesh.MeshSizeFactor": "{size_factor}"}}}}}
      },
      {"key": "size_boundary", "class": "action.Feature",
