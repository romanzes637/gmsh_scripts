      },
      {"key": "cast_iron", "class": "action.Feature",
        "sub_actions": [
          {"key": "size", "class": "action.Feature",
            "pre_call": {"class": "action.set.Continuous", "low": 0.01, "high": 0.1}}
        ],
        "post_call": {"class": "action.get.Json", "path": "gmsh/cast_iron.json",
          "mapping": {"data": {"layer": [
            ["1.450;{cast_iron.size}"],
            ["5.500;{cast_iron.size}"]]}}}
      },
      {"key": "filling", "class": "action.Feature",
        "sub_actions": [
          {"key": "size", "class": "action.Feature",
            "pre_call": {"class": "action.set.Continuous", "low": 0.01, "high": 0.1}}
