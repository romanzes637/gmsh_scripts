          "mapping": {"metadata": {"run": {"options": {"Mesh.MeshSizeFromCurvature": "{size_curvature}"}}}}}
      },
      {"key": "size_min", "class": "action.Feature",
        "pre_call": {"class": "action.set.Continuous", "low": 0, "high": 0.1},
