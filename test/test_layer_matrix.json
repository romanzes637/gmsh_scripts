{
  "metadata": {
    "issues": ["Issue: PCL Error: A segment and a facet intersect at point. Solution: Set appropriate mesh size or use MeshSizeFromCurvature See: https://gitlab.onelab.info/gmsh/gmsh/-/issues/766"],
    "run": {
      "factory": "occ",
      "log_level": "DEBUG",
      "strategy": {
        "class": "strategy.Base",
        "boolean_function": "boolean.BooleanAllBlock",
        "structure_function": "structure.StructureBlock",
        "quadrate_function": "quadrate.NoQuadrate",
        "size_function": "size.NoSize",
        "zone_function": {
          "class": "zone.DirectionByInterval",
          "dims_interfaces": []},
        "optimize_function": {
          "class": "optimize.OptimizeMany",
          "optimizers": [],
          "force": false,
          "threshold": 0.3},
        "refine_function": {
          "class": "refine.RefineBySplit",
          "n_iterations": 0},
        "smooth_function": {
          "class": "smooth.SmoothByDim",
          "dims": [0, 1, 2, 3],
          "n_iterations": 0,
          "smooth_normals": false,
          "smooth_cross_field": false}
      },
      "options": {
        "Geometry.OCCParallel": 1,
        "Mesh.Algorithm": 6,
        "Mesh.Algorithm3D": 1,
        "Mesh.SubdivisionAlgorithm": 2,
        "Mesh.RecombinationAlgorithm": 1,
        "Mesh.RecombineAll": 0,
        "Mesh.RecombineOptimizeTopology": 5,
        "Mesh.Recombine3DAll": 0,
        "Mesh.Recombine3DLevel": 0,
        "Mesh.Recombine3DConformity": 0,
        "Mesh.MeshSizeFactor": 1,
        "Mesh.MeshSizeMin": 0,
        "Mesh.MeshSizeMax": 1e+22,
        "Mesh.MeshSizeFromPoints": 1,
        "Mesh.MeshSizeExtendFromBoundary": 1,
        "Mesh.MeshSizeFromCurvature": 10,
        "Mesh.MeshSizeFromCurvatureIsotropic": 0,
        "Mesh.MeshSizeFromParametricPoints": 0,
        "Mesh.MinimumCircleNodes": 7,
        "Mesh.MinimumCurveNodes": 3
      }
    }
  },
  "data": {
    "class": "block.Matrix",
    "grid": [[0, "15:3;1;3"], [-7, "7:3;1;3"], [0, "13:3;1;3"]],
    "boolean_level_map": 0,
    "quadrate_map": 0,
    "children": ["test/test_layer_layer.json", "test/test_layer_layer.json"],
    "children_transforms": [[[0, 0, 0, 0, 0, 1, 80], [5, -5, 0]], []],
    "zones": ["M"]
  }
